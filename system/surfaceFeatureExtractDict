FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}

surfaces
(
    rocket.stl
    {
        extractionMethod    extractFromSurface;

        extractFromSurfaceCoeffs
        {
            includedAngle   150;   // sharp edges
        }

        writeObj            yes;
    }
);
